{
  "name": "catchresult",
  "version": "0.0.1",
  "description": "Handle neverthrow's Result type with ease!",
  "module": "dist/index.js",
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "bunx tsup src/index.ts --format esm,cjs --dts",
    "prepublishOnly": "bun scripts/prepublish.js"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "neverthrow",
    "errors",
    "error-handling"
  ],
  "author": "Udbhav Balaji",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/udbhavbalaji/catchresult.git"
  },
  "bugs": {
    "url": "https://github.com/udbhavbalaji/catchresult/issues"
  },
  "homepage": "https://github.com/udbhavbalaji/catchresult/#readme",
  "devDependencies": {
    "@types/bun": "latest",
    "tsup": "^8.5.0"
  },
  "peerDependencies": {
    "neverthrow": "^8.2.0",
    "typescript": "^5"
  }
}
